URxvt.urgentOnBell: true

! to match gnome-terminal "Linux console" scheme
! foreground/background
URxvt*background: #111111
URxvt*foreground: #FFFFFF

urxvt*cursorColor: #4E9A06
urxvt*cursorBlink: false

URxvt*color0: #101010
URxvt*color1: #f13a21
URxvt*color2: #93f91d
URxvt*color3: #ffd00a
URxvt*color4: #004f9e
URxvt*color5: #ec0048
URxvt*color6: #2aa7e7
URxvt*color7: #f2f2f2
URxvt*color8: #1d202f
URxvt*color9: #ff361e
URxvt*color10: #ffc005
URxvt*color11: #93ff00
URxvt*color12: #0071ff

URxvt*font: xft:Monospace:pixelsize=12 antialias=true

! ScrollBar
urxvt*scrollBar: true
urxvt*scrollBar_right: true
urxvt*scrollColor: #A2A2A2
urxvt*scrollBar_floating: true

! Transparency
!URxvt.inheritPixmap: true
!URxvt.tintColor: #330000
!URxvt.shading:100

! open url in firfox
URxvt.perl-ext-common : default,matcher
URxvt.urlLauncher     : firefox
URxvt.matcher.button  : 1

! transparencei
! URxvt*transparent: true
! URxvt*shading: 10

! Colors
URxvt*termName: screen-256color


Xcursor.theme: Vanilla-DMZ
